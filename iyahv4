echo '###################################################### DEBUGGING TIME ######################################################' &&
echo '############################################################################################################################' \e[0m" && read &&
make mrproper && export USE_CCACHE=1 && ./sbuild.sh galaxys s ; then timer && echo -e "\e[31;42m 'BUILD IYAHMANKERNEL DONE' \e[0m"
else spd-say 'bug' && echo -e "\e[31;42m '############################################################################################################################'
echo '###################################################### KERNEL INTERVENTION MANUEL REQUISE ######################################################' 
echo '############################################################################################################################' \e[0m" && read
fi

#****************************************************************
## Save Unofficial Nightly :
if echo -e "\e[31;42m 'SAVE UNOFFICIAL NIGHTLY' \e[0m" &&
cd ~/android/rom/iyahmanrom11.0/out/target/product/$VARIANT/ &&
cp -rfavu cm-IyahmanRom11.0.zip cm-11.0-`date -I`-UNOFFICIAL-$VARIANT.zip
then echo -e "\e[31;42m 'SAVE UNOFFICIAL NIGHTLY DONE' \e[0m"
else read
fi

#****************************************************************
## Move Rom And Extra Feature Into Temporaly Folder :
echo -e "\e[31;42m 'MOVING ROM, GAPPS INTO TEMPORALY FOLDER' \e[0m"
cd ~/android/rom/iyahmanrom11.0/out/target/product/$VARIANT/
