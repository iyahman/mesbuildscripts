#cp -rfavu ~/android/linaro/gcc-linaro/android-toolchain-eabi/* ~/android/rom/iyahmanrom11.0/prebuilts/gcc/linux-x86/arm/arm-eabi-4.6
#cp -rfavu ~/android/linaro/gcc-linaro/android-toolchain-eabi/* ~/android/rom/iyahmanrom11.0/prebuilts/gcc/linux-x86/arm/arm-linux-androideabi-4.6
#rm -rfv ~/android/rom/iyahmanrom11.0/prebuilts/gcc/linux-x86/arm/*

#****************************************************************
## config compiler for cyanogen kernel :
#if echo -e "\e[31;42m 'USE CUSTOM TOOLCHAIN FOR CYANOGEN KERNEL'  \e[0m" &&
#cd ~/android/rom/iyahmanrom11.0/kernel/samsung/aries &&
#make mrproper &&
#echo "CONFIG_S5P_HUGEMEM=y" >> .config &&
#export ARCH=arm &&
#export CROSS_COMPILE=~/android/linaro/linaro-toolchains/bin/arm-cortex_a8-linux-gnueabi- 
#then echo -e "\e[31;42m 'USE CUSTOM TOOLCHAIN FOR CYANOGEN KERNEL DONE' \e[0m"
#else read
#fi

#****************************************************************
##Add Cherry Pick :

#****************************************************************
##replace original system syze by Iyah 
#if [ "$VARIANT" = "galaxysmtd" ] ; then cd ~/android/rom/iyahmanrom11.0/device/samsung/aries-common
#sed -i 's/629145600/304087040/g' updater.sh ; fi

#****************************************************************
##Build CM10 :
if echo -e "\e[31;42m 'BUILD CM11.0 ROM' \e[0m" &&
cd ~/android/rom/iyahmanrom11.0/ &&
export USE_CCACHE=1 && . build/envsetup.sh && brunch $VARIANT ; then
timer && echo -e "\e[31;42m 'BUILD CM11.0 ROM DONE' \e[0m"
elif echo -e "\e[31;42m '############################################################################################################################' &&
echo '###################################################### DEBUGGING TIME ######################################################' &&
echo '############################################################################################################################' \e[0m" && read &&
make clean && export USE_CCACHE=1 && . build/envsetup.sh && brunch $VARIANT
then timer && echo -e "\e[31;42m 'BUILD CM11.0 ROM DONE' \e[0m"
elif rm -rfv ~/android/rom/iyahmanrom11.0/out/target/product/$VARIANT &&
make clean && export USE_CCACHE=1 && . build/envsetup.sh && brunch $VARIANT
then timer && echo -e "\e[31;42m 'BUILD CM11.0 ROM DONE' \e[0m" ; else spd-say 'bug' && echo timer  
echo -e "\e[31;42m '############################################################################################################################'
echo '###################################################### ROM INTERVENTION MANUEL REQUISE ######################################################' 
echo '############################################################################################################################' \e[0m" && read
fi

#****************************************************************
##Build Semaphore :
if [ "$VARIANT" = "galaxysmtd" ] && echo  -e "\e[31;42m'BUILD IYAHMANKERNEL' \e[0m" && 
cd /home/iyahman/android/rom/semaphore/11.0/samsung-kernel-aries/ &&
#cp -rfavu /home/iyahman/android/rom/iyahmanrom11.0/out/target/product/$VARIANT/recovery/root/sbin/recovery /home/iyahman/android/rom/semaphore/ics-ramdisk/jb_combo/ics_rec_init/sbin/
