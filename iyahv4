#****************************************************************
## Delete Unwanted Apk :
echo -e "\e[31;42m 'DELETING UNWANTED STUFF' \e[0m"
cd ~/android/rom/buildtemporaire/IyahmanRom11.0Extract/system/app/
if [ "$VARIANT" != "jflte" ] ; then
#dualboot apks
rm -rfv com.github.chenxiaolong.dualbootpatcher.apk
