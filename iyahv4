cd ~/android/rom/buildtemporaire/ &&
rm -rfv cm-IyahmanRom11.0.zip &&
cd IyahmanRom11.0Extract &&
zip -qrv ../cm-IyahmanRom11.0.zip ./ ; then
echo -e "\e[31;42m 'ZIPPING IYAHMANROM DONE' \e[0m"
fi

#****************************************************************
## Move rom To Final Folder :
if echo -e "\e[31;42m 'MOVING IYAHMANROM INTO FINAL DIRECTORY' \e[0m" &&
cd ~/android/rom/buildtemporaire/ &&
mv -rfv cm-IyahmanRom11.0.zip ~/android/rom/iyahmanrom11.0/out/target/product/$VARIANT/
then echo -e "\e[31;42m 'MOVING IYAHMANROM INTO FINAL DIRECTORY DONE' \e[0m" ; else read
fi

#****************************************************************
## Rename Rom :
if echo -e "\e[31;42m 'RENAMING BUILD' \e[0m" && 
cd ~/android/rom/iyahmanrom11.0/out/target/product/$VARIANT/ &&
mv -rfv cm-IyahmanRom11.0.zip IyahmanRom11.0$VER-`date -I`.zip
then echo -e "\e[31;42m 'RENAMING BUILD DONE' \e[0m" ; else read 
fi

#****************************************************************
## Dual Boot :
if [ "$VARIANT" = "jflte" ] && echo -e "\e[31;42m 'PATCH FOR DUALBOOT I9505' \e[0m" ; then cd ~/android/rom/iyahmanrom11.0/out/target/product/jflte/ &&
cp -f IyahmanRom11.0$VER-`date -I`.zip cm-11-`date -u +%Y%m%d`-NIGHTLY-jflte.zip &&
cd ~/android/DualBootPatcher &&
./patch-file.sh /home/iyahman/android/rom/iyahmanrom11.0/out/target/product/$VARIANT/cm-11-`date -u +%Y%m%d`-NIGHTLY-jflte.zip &&
cd ~/android/rom/iyahmanrom11.0/out/target/product/$VARIANT/ &&
mv -f cm-11-`date -u +%Y%m%d`-NIGHTLY-jflte_dualboot.zip IyahmanRom11.0$VER-`date -I`"_dualboot".zip &&
rm -f cm-11-`date -u +%Y%m%d`-NIGHTLY-jflte.zip && timer &&
echo -e "\e[31;42m 'PATCH FOR DUALBOOT I9505 DONE' \e[0m" ; else read
fi

#****************************************************************
## Sign cm-IyahmanRom11.0 :
if echo -e "\e[31;42m 'SIGNING ZIP' \e[0m" && cd ~/android/rom/iyahmanrom11.0/out/target/product/$VARIANT/ &&
openssl pkcs8 -in /home/iyahman/android/rom/iyahmanrom11.0/build/target/product/security/testkey.pk8 -inform DER -nocrypt &&
java -Xmx2048m -jar /home/iyahman/android/rom/iyahmanrom11.0/out/host/linux-x86/framework/signapk.jar -w ~/android/rom/iyahmanrom11.0/build/target/product/security/testkey.x509.pem ~/android/rom/iyahmanrom11.0/build/target/product/security/testkey.pk8 IyahmanRom11.0$VER-`date -I`.zip IyahmanRom11.0$VER-`date -I`-signed.zip
then timer && echo -e "\e[31;42m 'SIGNING ZIP DONE' \e[0m" ; else read
fi
if [ "$VARIANT" = "jflte" ] && echo -e "\e[31;42m 'SIGNING ZIP' \e[0m" &&
cd ~/android/rom/iyahmanrom11.0/out/target/product/$VARIANT/ &&
openssl pkcs8 -in /home/iyahman/android/rom/iyahmanrom11.0/build/target/product/security/testkey.pk8 -inform DER -nocrypt &&
java -Xmx2048m -jar /home/iyahman/android/rom/iyahmanrom11.0/out/host/linux-x86/framework/signapk.jar -w ~/android/rom/iyahmanrom11.0/build/target/product/security/testkey.x509.pem ~/android/rom/iyahmanrom11.0/build/target/product/security/testkey.pk8 IyahmanRom11.0$VER-`date -I`"_dualboot".zip IyahmanRom11.0$VER-`date -I`"_dualboot"-signed.zip
then timer && echo -e "\e[31;42m 'SIGNING DUALBOOT ZIP DONE' \e[0m" ; else read
fi

#****************************************************************
## Zipalign cm-IyahmanRom11.0 :
if echo -e "\e[31;42m 'ZIPALIGNING' \e[0m" &&
cd ~/android/sdk/tools/ &&
#zipalign -c -v 4 ~/android/rom/iyahmanrom11.0/out/target/product/$VARIANT/IyahmanRom11.0$VER-`date -I`-signed.zip
zipalign -f -v 4 ~/android/rom/iyahmanrom11.0/out/target/product/$VARIANT/IyahmanRom11.0$VER-`date -I`-signed.zip ~/android/rom/iyahmanrom11.0/out/target/product/$VARIANT/IyahmanRom11.0$VER-`date -I`-signed-zipaligned.zip
then timer && echo -e "\e[31;42m 'ZIPALIGNING DONE' \e[0m" ; else read
fi
if [ "$VARIANT" = "jflte" ] && cd ~/android/sdk/tools/ &&
#zipalign -c -v 4 ~/android/rom/iyahmanrom11.0/out/target/product/$VARIANT/IyahmanRom11.0$VER-`date -I`"_dualboot".zip
zipalign -f -v 4 ~/android/rom/iyahmanrom11.0/out/target/product/$VARIANT/IyahmanRom11.0$VER-`date -I`"_dualboot"-signed.zip ~/android/rom/iyahmanrom11.0/out/target/product/$VARIANT/IyahmanRom11.0$VER-`date -I`"_dualboot"-signed-zipaligned.zip
then timer && echo -e "\e[31;42m 'ZIPALIGNING  DUALBOOT DONE' \e[0m" ; else read
fi

#****************************************************************
## Clean buildtemporaire:
echo -e "\e[31;42m 'CLEANING TEMPORALY FILE' \e[0m"
cd ~/android/rom/buildtemporaire/
rm -rf IyahmanRom11.0Extract
rm -rf cm-IyahmanRom11.0.zip
rm -rf gapps11.0Extract
rm -rf gapps11.0.zip
rm -rf bootanimationExtract
rm -rf bootanimation.zip
cd ~/android/rom/iyahmanrom11.0/out/target/product/$VARIANT/
rm -rf cm-IyahmanRom11.0.zip.md5sum
if [ "$VARIANT" = "galaxysmtd" ] ; then
rm -rf patch720p4.4Extract
rm -rf patch720p4.4.zip
rm -rf IyahmanKernelExtract
rm -rf IyahmanKernels.zip
rm -rf modemril.zip
rm -rf modemrilExtract
cd ~/android/rom/semaphore/11.0/output/galaxys
rm -rf IyahmanKernelsOld.zip
rm -rf IyahmanKernelsOld.tar
fi
echo -e "\e[31;42m 'CLEANING TEMPORALY FILE DONE' \e[0m"
echo -e "\e[31;42m '############################################################################################################################' \e[0m"
echo -e "\e[31;42m '###################################################### BUILD TERMINÉ ######################################################' \e[0m"
echo -e "\e[31;42m '############################################################################################################################' \e[0m"

#****************************************************************
## Report TOTAL timer:
DATE_END=$(date +"%s")
echo
DIFF=$(($DATE_END - $DATE_START))
echo {CM11.0 POUR $VARIANT FINI EN} "$(($DIFF / 60)) minute(s) and $(($DIFF % 60)) seconds."

#****************************************************************
## Open Final Build Directory:
spd-say 'BUILD SUCCESFULL' && nautilus ~/android/rom/iyahmanrom11.0/out/target/product/$VARIANT/

bash -c "sleep 5m; pm-suspend-hybrid"
#exit
