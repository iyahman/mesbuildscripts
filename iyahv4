#magic smoke wallpaper
rm -rfv MagicSmokeWallpapers.apk
#movie studio
rm -rfv VideoEditor.apk
#navigateur
rm -rfv Browser.apk
#quicksearhbox
rm -rfv QuickSearchBox.apk
#phase beam
rm -rfv PhaseBeam.apk
# Gallery cm11
rm -rfv Gallery2.apk
# camera cm11
rm -rfv Camera2.apk
cd ~/android/rom/buildtemporaire/IyahmanRom11.0Extract/system/priv-app/
#cm updater
rm -rfv CMUpdater.apk
#pico tts
rm -rfv PicoTts.apk
#reconnaissance vocal
rm -rfv VoiceDialer.apk
#trebuchet
rm -rfv Trebuchet.apk
